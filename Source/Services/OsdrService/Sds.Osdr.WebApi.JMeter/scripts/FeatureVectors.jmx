import net.minidev.json.JSONArray;
import net.minidev.json.JSONObject;
import net.minidev.json.parser.JSONParser;
import org.apache.commons.lang.StringUtils;
import java.util.*;

log.info(&quot;-------------- BEGIN --------------&quot;);
String valueFingerprints = JMeterUtils.getPropDefault(&quot;fingerprints&quot;,&quot;&quot;);


//log.info(&quot;valueFingerprints:: &quot; + valueFingerprints);

JSONParser parser = new JSONParser(JSONParser.MODE_JSON_SIMPLE);
JSONObject fingerprints = (JSONObject) parser.parse(valueFingerprints);

var fingerprintSize = fingerprints.get(&quot;fingerprints&quot;).size();
var fingerprintIndex = ctx.getThreadNum() % fingerprintSize;

log.info(&quot;::&gt; index: &quot; + fingerprintIndex);

vars.put(&quot;index&quot;, fingerprintIndex.toString());

var fingerprint = fingerprints.get(&quot;fingerprints&quot;).get(fingerprintIndex);
var paramSize = fingerprints.get(&quot;radius&quot;).get(0).get(&quot;value&quot;);
var paramRadius = fingerprints.get(&quot;size&quot;).get(0);

log.info(&quot;::: &quot; + fingerprint);

Boolean isSize = false;
Boolean isRadius = false;

var params = fingerprint.get(&quot;parameters&quot;).toString();
var paramKey = fingerprint.get(&quot;key&quot;).toString();

//log.info(&quot;fingerprint size: &quot; + paramSize);
//log.info(&quot;fingerprint radius: &quot; + paramRadius);

String parameter = &quot;[{\&quot;type\&quot;:\&quot;&quot; + paramKey + &quot;\&quot;&quot;;

// empty params
if (params.equals(&quot;{}&quot;))
{
	log.info(&quot;params is empty&quot;);
	isSize = false;
	isRadius = false;
	vars.put(&quot;fingerprint_size&quot;, &quot;&quot;);
	vars.put(&quot;fingerprint_radius&quot;, &quot;&quot;);

	parameter += &quot;}]&quot;;
} else {
	log.info(&quot;params is fill&quot;);
	isSize = Boolean.parseBoolean(fingerprint.get(&quot;parameters&quot;).get(&quot;size&quot;).toString());
	isRadius = Boolean.parseBoolean(fingerprint.get(&quot;parameters&quot;).get(&quot;radius&quot;).toString());

	if (isSize)
	{
		parameter += &quot;,\&quot;size\&quot;:&quot; + paramSize;
		vars.put(&quot;fingerprint_size&quot;, paramSize.toString());
	} else {
		vars.put(&quot;fingerprint_size&quot;, &quot;&quot;);
	}

	if (isRadius)
	{
		parameter += &quot;,\&quot;radius\&quot;:&quot; + paramRadius;
		vars.put(&quot;fingerprint_radius&quot;, paramRadius.toString());
	} else {
		vars.put(&quot;fingerprint_radius&quot;, &quot;&quot;);
	}
	
	parameter += &quot;}]&quot;;
}

log.info(&quot;key: &quot; + paramKey);
vars.put(&quot;fingerprint_type&quot;, paramKey.toString());

log.info(&quot;params { size: &quot; + isSize + &quot;, radius: &quot; + isRadius + &quot;}&quot;);
log.info(&quot;parameter: &quot; + parameter);

vars.put(&quot;parameter&quot;, parameter);
</stringProp>
